package fifth;

import java.util.Vector;

public class Person {
	Gender gender;
	String name;

	
	public Person(){
		
	}
	public Person(String name) {
		this();
		this.name = name;
	}
	public Person(String name, Gender gender) {
		this(name);
		this.gender = gender;
	}
	
	public String toString() {
		return name + "" + gender;
	}
	
	
	
}

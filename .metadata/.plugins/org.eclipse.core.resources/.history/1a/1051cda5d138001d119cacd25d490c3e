package second;

public class Dataaa {
	DAYs d = DAYs.SAT;
	final double price ;
	private static int id;
	public String productName;
	public String color;
	
	{
		price = 14.99;
		id = 0;
	}
	
	public Dataaa() {
		id++;
	}
	public Dataaa(String productName) {
		this();
		this.productName = productName;
	}
	public Dataaa(String productName, String color) {
		this(productName);
		this.color = color;
	}
	
	
}

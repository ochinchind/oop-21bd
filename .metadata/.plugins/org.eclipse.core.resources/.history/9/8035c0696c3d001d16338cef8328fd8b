package fifth;

import java.util.Vector;

public class Person {
	Gender gender;
	String name;
	static Vector<Gender> students = new Vector<Gender>();
	
	public Person(){
		
	}
	public Person(String name) {
		this();
		this.name = name;
	}
	public Person(String name, Gender gender) {
		this(name);
		students.add(gender);
	}
	
	public String toString() {
		return "" + gender;
	}
	
	
	
}

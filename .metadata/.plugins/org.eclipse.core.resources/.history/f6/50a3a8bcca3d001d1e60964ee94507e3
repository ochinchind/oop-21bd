package fifth;

import java.util.Vector;

public class Person {
	Gender gender;
	Vector <String> kidnap = new Vector<String>();

	
	public Person(){
		
	}
	public Person(Gender gender) {
		this.gender = gender;
	}

	public String toString() {
		return ""+gender;
	}
	public void pushBoy() {
		kidnap.add("B");
	}
	public void pushGirl() {
		kidnap.add("G");
	}
	
	
	
}
